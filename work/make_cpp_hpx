mv ./odir4/dgswem.o ./odir4/dgswem.oB
INCLUDES=`ls -d -1 ./odir4/*.o`
echo $INCLUDES
mv ./odir4/dgswem.oB ./odir4/dgswem.o
CMD="g++ -c ../src/dgswem_cpp.cpp -DHPX `pkg-config --cflags --libs hpx_application`"
$CMD 
CMD="g++ -o dgswem_hpx_cpp dgswem_cpp.o $INCLUDES -DHPX `pkg-config --cflags --libs hpx_application` -lgfortran"
echo $CMD
$CMD